				<Property Name="DistanceFromHome" Type="Edm.Int32"/>
				<Property Name="Education" Type="Edm.String" MaxLength="50"/>
				<Property Name="EducationField" Type="Edm.String" MaxLength="50"/>
				<Property Name="EmployeeCount" Type="Edm.Int32"/>
				<Property Name="Gender" Type="Edm.String" MaxLength="50"/>
