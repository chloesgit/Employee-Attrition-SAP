				<Property Name="JobSatisfaction" Type="Edm.Int32"/>
				<Property Name="WorkLifeBalance" Type="Edm.Int32"/>
				<Property Name="JobInvolvement" Type="Edm.Int32"/>
				<Property Name="PerformanceRating" Type="Edm.Int32"/>
			</EntityType>
			<EntityContainer Name="v2" m:IsDefaultEntityContainer="true">
				<EntitySet Name="employees" EntityType="default.employeesType"/>
			</EntityContainer>
		</Schema>
	</edmx:DataServices>
</edmx:Edmx>